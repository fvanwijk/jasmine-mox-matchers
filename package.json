{
  "name": "jasmine-mox-matchers",
  "version": "0.3.1",
  "author": "Frank van Wijk <fvanwijk@xebia.com>",
  "homepage": "https://github.com/fvanwijk/jasmine-mox-matchers",
  "repository": {
    "type": "git",
    "url": "git://github.com/fvanwijk/jasmine-mox-matchers.git"
  },
  "dependencies": {
    "angular": "1.6.2",
    "lodash": "4.17.4"
  },
  "devDependencies": {
    "angular-mocks": "1.6.2",
    "del-cli": "0.2.1",
    "eslint": "3.15.0",
    "eslint-config-airbnb-base": "11.1.0",
    "eslint-plugin-import": "2.2.0",
    "jasmine-core": "2.5.2",
    "karma": "1.4.1",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "1.1.0",
    "karma-phantomjs-launcher": "1.0.2",
    "phantomjs-prebuilt": "2.1.14",
    "test-runner-config": "0.5.0",
    "testception": "1.0.1"
  },
  "scripts": {
    "build": "mkdir dist && uglifyjs src/jasmine-mox-matchers.js -o ./dist/jasmine-mox-matchers.min.js",
    "clean": "del dist coverage",
    "lint:js": "eslint . --color",
    "qa": "npm run lint:js && npm run test:unit",
    "prepublish": "npm run clean && npm run qa && npm run build",
    "preversion": "npm run qa",
    "test:unit": "karma start"
  },
